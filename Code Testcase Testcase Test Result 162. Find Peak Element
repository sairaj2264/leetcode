class Solution {
public:
    int findPeakElement(vector<int>& nums) {
        
        long long n = nums.size();
        long long index = 0;
        long long maxx = INT_MIN;

        for(int i=0;i<n;i++){

            if(nums[i]>maxx){
                maxx = nums[i];
                index = i;
            }

    }
        return index;
    }


};
